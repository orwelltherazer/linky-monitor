{
  "name": "linky-monitor",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "node server.js",
    "full": "concurrently \"npm run start\" \"npm run dev\"",
    "deploy": "npm run build && npm start",
    "backup": "node -e \"import('./server/backup.js').then(m => m.default.createBackup())\"",
    "restore": "node -e \"import('./server/backup.js').then(m => console.log('Backups disponibles:', m.default.listBackups()))\"",
    "migrate": "node -e \"import('./server/migration.js').then(m => m.default.migrate())\"",
    "cron:update": "node cron-updater.js update",
    "cron:historical": "node cron-updater.js historical"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^4.21.1",
    "lucide-react": "^0.553.0",
    "mysql2": "^3.11.5",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "recharts": "^3.4.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.0.0",
    "@tailwindcss/vite": "^4.1.17",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.0",
    "vite": "^7.2.2"
  }
}
